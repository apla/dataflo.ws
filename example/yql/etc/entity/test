[
	{
		"$function":"Array",
		"$args":[
			[ "data", "must", "flow" ],
			[ "in", "nested", "loops" ]
		],
		"$set":"testArr"
	},
	{
		"$every":"{$testArr}",
		"$collect":"result1",
		"$tasks":[{
			"$every":"{#every.item}",
			"$collect":"result2",
			"$tasks":[{
				"$function":"toUpperCase",
				"$origin":"{#every.item}",
				"$set":"result2"
			},
			{
				"$function":"console.print",
				"$args":"{$request.url.path}"
			},
			{
				"$function":"console.print",
				"$args":[
					"Running $every: index: {#every.index},",
					"item: {#result2}",
					"array: {$testArr}"
				]
			}],
			"$set":"result1"
		}],
		"$set":"mappedArr"
	},
	{
		"$function":"Array.prototype.join",
		"$args":[" "],
		"$scope":"{$mappedArr}",
		"$set":"resultMessage"
	}
]
